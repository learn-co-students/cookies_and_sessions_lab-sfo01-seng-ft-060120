<h3>Add to your cart:</h3>
<%= form_tag add_to_cart_path do %>
    <label>Product:</label>
    <%= text_field_tag :product %>

    <%= submit_tag "add to cart"%>
<% end %>

<h3>Items in your cart:</h3>
<ul>
    <% cart.each do |item| %>
     <% if item %>
        <li> <%= item %> </li>
    <% end %>
    <% end %>
</ul>